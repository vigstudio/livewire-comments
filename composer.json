{
    "name": "vigstudio/livewire-comments",
    "description": "Livewire comments packages use backend by vgcomments",
    "keywords": [
        "vigstudio",
        "livewire-comments",
        "laravel-comments",
        "comments",
        "comments-system",
        "livewire"
    ],
    "homepage": "https://github.com/vigstudio/livewire-comments",
    "license": "MIT",
    "type": "laravel-library",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Nghiane.com",
            "email": "vingadev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "vigstudio/vgcomments": "^1.1.0",
        "livewire/livewire": "^2.10.7",
        "archielite/laravel-heroicons": "^0.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vigstudio\\LivewireComments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigstudio\\LivewireComments\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vigstudio\\LivewireComments\\LivewireCommentsServiceProvider"
            ],
            "aliases": {}
        }
    }
}